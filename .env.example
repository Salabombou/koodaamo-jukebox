# Server Environment Variables

# Database connection string (ex. Server=localhost;Database=mydb;User Id=myuser;Password=mypassword;)
CONNECTION_STRING=YOUR_DATABASE_CONNECTION_STRING
# Secret key for signing JWT tokens (ex. my_super_secret_key)
JWT_SECRET=YOUR_JWT_SECRET
# Discord application's client secret (ex. abcdef1234567890abcdef1234567890)
DISCORD_CLIENT_SECRET=YOUR_DISCORD_CLIENT_SECRET
# Discord application's client ID (ex. 123456789012345678)
DISCORD_CLIENT_ID=YOUR_DISCORD_CLIENT_ID
# Discord OAuth2 redirect URI (ex. http://localhost:3000/auth/discord/callback)
DISCORD_REDIRECT_URI=YOUR_DISCORD_REDIRECT_URI
# YouTube Data API v3 key (ex. AIzaSyA...)
YOUTUBE_API_KEY=YOUR_YOUTUBE_V3_API_KEY
# yt-dlp executable path (ex. /usr/local/bin/yt-dlp)
YT_DLP_PATH=YOUR_YT_DLP_PATH


# Client Environment Variables

# Discord application ID for the client (should match server DISCORD_CLIENT_ID) (ex. 123456789012345678)
VITE_DISCORD_APPLICATION_ID=YOUR_DISCORD_APPLICATION_ID
# Discord OAuth2 URL for the client (ex. https://discord.com/oauth2/authorize?...)
VITE_DISCORD_OAUTH2_URL=YOUR_DISCORD_OAUTH2_URL


# Bot Environment Variables

# Discord bot token (ex. MTAx...)
DISCORD_BOT_TOKEN=YOUR_DISCORD_BOT_TOKEN
# Base URL for the API (ex. http://localhost:5000/api)
API_BASE_URL=YOUR_API_BASE_URL
# Base URL for the production API (ex. https://api.example.com)
API_BASE_URL_PROD=YOUR_API_BASE_URL_PROD
# Secret key for signing JWT tokens (should match server JWT_SECRET) (ex. my_super_secret_key)
JWT_SECRET=YOUR_JWT_SECRET
